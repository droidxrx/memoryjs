{
	"name": "memoryjs",
	"version": "3.3.1",
	"description": "Node add-on for memory reading and writing!",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"install": "node-gyp rebuild --arch=x64 --release",
		"build-ts": "rollup -c rollup.config.mjs",
		"build-dts": "dts-bundle-generator -o dist/memoryjs.d.ts --project ./tsconfig.json src/index.ts --no-banner"
	},
	"keywords": [
		"memory",
		"reading",
		"writing",
		"management",
		"addon"
	],
	"author": "Rob--",
	"license": "MIT",
	"gypfile": true,
	"dependencies": {
		"node-addon-api": "^4.3.0"
	},
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^13.1.3",
		"@types/bindings": "^1.5.1",
		"@types/node": "^17.0.21",
		"@typescript-eslint/eslint-plugin": "^5.14.0",
		"@typescript-eslint/parser": "^5.14.0",
		"eslint": "^8.11.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.25.4",
		"rollup": "^2.70.1",
		"rollup-plugin-ts": "^2.0.5",
		"typescript": "^4.6.2"
	},
	"private": true,
	"engines": {
		"node": ">=16.14.0"
	},
	"os": [
		"win32"
	],
	"cpu": [
		"x64"
	],
	"clean-publish": {
		"files": [
			"build",
			"example",
			"external",
			"src",
			".gitignore",
			".eslintrc.js",
			"rollup.config.mjs",
			"tsconfig.json"
		],
		"withoutPublish": true,
		"tempDir": "package"
	}
}

